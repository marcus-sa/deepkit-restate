    "@deepkit/logger": "^1.0.15",
    "@deepkit/type": "^1.0.15",
    "@deepkit/type-compiler": "^1.0.15",
    "@deepkit/vite": "^1.0.15",
    "@restatedev/restate-sdk": "1.8.1",
    "@restatedev/restate-sdk-clients": "1.8.1",
    "@restatedev/restate-sdk-testcontainers": "1.8.1",
    "@swc/core": "^1.12.7",
    "@types/bun": "^1.2.17",
    "prettier": "^3.6.2",
    "typescript": "5.7.3",
    "vite": "7.0.0",
    "vitest": "3.2.4"
  },
