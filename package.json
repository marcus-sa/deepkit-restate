{
  "name": "deepkit-restate",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "https://github.com/marcus-sa/deepstate.git",
  "homepage": "https://deepstate.js.org",
  "author": "Marcus S. Abildskov <work@marcus-sa.dev>",
  "type": "module",
  "main": "./lib/cjs/src/index.js",
  "module": "./lib/esm/src/index.js",
  "types": "./lib/types/src/index.d.js",
  "exports": {
    ".": {
      "require": "./lib/cjs/src/index.js",
      "import": "./lib/esm/src/index.js",
      "types": "./lib/types/src/index.d.js"
    },
    "./kafka": {
      "require": "./lib/cjs/src/kafka/index.js",
      "import": "./lib/esm/src/kafka/index.js",
      "types": "./lib/types/src/kafka/index.d.js"
    },
    "./event-server": {
      "require": "./lib/cjs/src/event/server/index.js",
      "import": "./lib/esm/src/event/server/index.js",
      "types": "./lib/types/src/event/server/index.d.js"
    }
  },
  "files": [
    "README.md",
    "lib"
  ],
  "scripts": {
    "postinstall": "deepkit-type-install",
    "build": "rm -rf lib && tsc -p tsconfig.json",
    "test": "rm -rf test && tsc -p tsconfig.spec.json && cd test && bun test",
    "prepublish": "bun run build",
    "check": "biome check",
    "check:fix": "biome check --fix --unsafe",
    "check:write": "biome check --write"
  },
  "dependencies": {
    "@restatedev/restate-sdk": "^1.4.0",
    "@restatedev/restate-sdk-clients": "^1.4.0",
    "kafkajs": "^2.2.4",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "@deepkit/app": "^1.0.2",
    "@deepkit/broker": "^1.0.2",
    "@deepkit/bson": "^1.0.2",
    "@deepkit/core": "^1.0.1",
    "@deepkit/event": "^1.0.2",
    "@deepkit/filesystem": "^1.0.1",
    "@deepkit/framework": "^1.0.2",
    "@deepkit/injector": "^1.0.2",
    "@deepkit/type": "^1.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@deepkit/bun": "^1.0.2",
    "@deepkit/type-compiler": "^1.0.2",
    "@types/bun": "^1.2.4",
    "typescript": "5.7.3"
  }
}
