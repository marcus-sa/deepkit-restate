    "@deepkit/filesystem": "^1.0.15",
    "@deepkit/framework": "^1.0.15",
    "@deepkit/http": "^1.0.15",
    "@deepkit/injector": "^1.0.15",
    "@deepkit/type": "^1.0.15",
    "kafkajs": "^2.2.4"
  },
  "devDependencies": {
    "@deepkit/app": "^1.0.15",
    "@deepkit/broker": "^1.0.15",
    "@deepkit/bson": "^1.0.15",
    "@deepkit/bun": "^1.0.15",
    "@deepkit/core": "^1.0.15",
    "@deepkit/event": "^1.0.15",
    "@deepkit/filesystem": "^1.0.15",
